@model BrandViewModel
<link href="~/ManagementDashboard/CSS/Brand/Create.css" rel="stylesheet" />
<script src="~/ManagementDashboard/JS/Brand/Create.js"></script>

<partial name="_CreateOrEdit" for="@Model" />

<div class="container m-3">
    <div class="row mx-0 px-0 align-items-center justify-content-center">
        <div class="my-1 col-md-3 col-6">
            <input type="button" id="createbtn" class="btn btn-block btn-primary" value="@Fields.SaveButton" formaction="@Constants.ControllerBrand/@Constants.ActionCreate" />
        </div>
        <div class="my-1 col-md-3 col-6">
            <input type="button" id="cancelCratebtn" class="btn btn-block btn-warning" value="@Fields.ClearButton" />
        </div>
    </div>
</div>