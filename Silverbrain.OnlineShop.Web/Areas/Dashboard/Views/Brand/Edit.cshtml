@model BrandViewModel
<link href="~/ManagementDashboard/CSS/Brand/Edit.css" rel="stylesheet" />
<script src="~/ManagementDashboard/JS/Brand/Edit.js"></script>

<partial name="_CreateOrEdit" model="@Model" />

<div class="container m-3">
    <div class="row mx-0 px-0 align-items-center justify-content-center">
        <div class="my-1 col-md-3 col-6">
            <input type="button" id="updatebtn" class="btn btn-block btn-outline-success" value="@Fields.SaveButton" formaction="@Constants.ControllerBrand/@Constants.ActionEdit" />
        </div>
        <div class="my-1 col-md-3 col-6">
            <input type="button" id="cancelUpdatebtn" class="btn btn-block btn-outline-danger" value="@Fields.CancelButton" />
        </div>
    </div>
</div>